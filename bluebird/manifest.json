{
  "manifest_version": 2,
 
  "name": "Bluebird",
  "description": "twitter image downloader",
  "version": "1.3.2",
 
  "browser_action": {
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "downloads",
    "background",
    "tabs",
    "https://twitter.com/*"
  ]
}